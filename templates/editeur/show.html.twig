{% extends 'base.html.twig' %}

{% block title %}Editeur{% endblock %}

{% block body %}
    <h1>Editeur</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ editeur.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ editeur.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ editeur.prenom }}</td>
            </tr>
            <tr>
                <th>DeletedAt</th>
                <td>{{ editeur.deletedAt ? editeur.deletedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ editeur.createdAt ? editeur.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ editeur.updatedAt ? editeur.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_editeur_index') }}">back to list</a>

    <a href="{{ path('app_editeur_edit', {'id': editeur.id}) }}">edit</a>

    {{ include('editeur/_delete_form.html.twig') }}
{% endblock %}
